# Local Development Environment Configuration
# Copy this file to local.env and fill in your values

# Environment
MOMENTUM_ENV=local
DEBUG=true

# Local PostgreSQL Database (for docker-compose)
DATABASE_URL=postgresql://momentum_user:momentum_password@localhost:5432/momentum_calc

# Alternative: Individual PostgreSQL settings
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=momentum_calc
POSTGRES_USER=momentum_user
POSTGRES_PASSWORD=momentum_password

# Application Settings
MAX_STOCKS=100
CACHE_TTL=3600

# Supabase Credentials (for testing cloud compatibility)
# These will be ignored in local environment
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
